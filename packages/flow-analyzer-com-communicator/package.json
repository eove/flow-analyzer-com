  "keywords": [
    "pf300",
    "citrex",
    "h4",
    "gaz",
    "flow",
    "analyzer"
  ],
  "bugs": {
    "url": "https://github.com/eove/flow-analyzer-com/issues"
  },
  "homepage": "https://github.com/eove/flow-analyzer-com#readme",
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/jest": "25.1.3",
    "@types/lodash": "4.14.149",
    "@types/node": "13.7.4",
    "@types/rx": "4.1.1",
    "@types/sinon": "7.5.1",
    "jest": "25.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.0.0",
    "sinon": "9.0.0",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.8.2"
  },
  "jest": {
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "/lib/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ]
  },
  "dependencies": {
    "@arpinum/messaging": "3.0.0",
    "@arpinum/promising": "2.2.3",
    "debug": "4.1.1",
    "lodash": "4.17.15",
    "rxjs": "6.5.4",
    "serialport": "10.3.0"
  }
